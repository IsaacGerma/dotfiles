-N 12
--alias get-audio,-X "-x --prefer-free-formats -o '~/Music/yt-dlp/%(channel)s/%(upload_date)s - %(title).64s.%(ext)s'"
--alias get-album "-x --prefer-free-formats -o '~/Music/yt-dlp/%(channel)s/%(playlist)s/%(title).64s.%(ext)s' --parse-metadata '%(video_autonumber)s:%(track_number)s' --parse-metadata '%(playlist)s:%(album)' --embed-metadata"
--alias dl-channel "-o '~/Videos/yt-dlp/%(uploader)s/%(upload_date)s - %(title)s.%(ext)s' --embed-thumbnail --embed-metadata -f 'bv*[height=720]+ba'"
